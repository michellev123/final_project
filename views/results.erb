            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Your customized packing list includes the following:</h2> <br>
                  
                  <p class="section-heading"> <%=@part1%>  </p><br><br><br><br>
                  
                  <% @pack_array.each do |item|%>
                  
                  <p   class="section-heading"> <%=item%> </p>
                  
                  <%end%>
                </div>
            </div>
          
          
 
  


  

   
          
          <footer>
